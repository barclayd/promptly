{
  "$schema": "node_modules/wrangler/config-schema.json",
  "name": "promptly",
  "compatibility_date": "2025-10-08",
  "compatibility_flags": ["nodejs_compat_v2"],
  "main": "./workers/app.ts",
  "observability": {
    "enabled": true
  },
  "d1_databases": [
    {
      "binding": "promptly",
      "database_name": "promptly",
      "database_id": "eadc5f7c-e195-4bd0-bcb1-8e4d2950606b",
      "migrations_dir": "./migrations/drizzle"
    }
  ],
  "kv_namespaces": [
    {
      "binding": "PROMPTS_CACHE",
      "id": "fe422f717c1b4a8295656d6e494161a3"
    }
  ],
  "durable_objects": {
    "bindings": [
      {
        "name": "PRESENCE_ROOM",
        "class_name": "PresenceRoom"
      }
    ]
  },
  "migrations": [
    {
      "tag": "v1",
      "new_sqlite_classes": ["PresenceRoom"]
    }
  ]
}
